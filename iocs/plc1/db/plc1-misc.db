record(bi,"XF:06BM-EPS{PLC}Fuse-Sts") {
    field(DESC, "PLC main unit 24V fuse blown")
    field(SCAN, "1 second")
    field(DTYP, "EtherIP")
    field(INP, "@$(PLC,undefined) Misc_Sts_0[7] S 1")
    field(ZNAM,"OK")
    field(ONAM,"Blown")
    field(ZSV,"")
    field(OSV,"MAJOR")
}

record(bi,"XF:06BM-EPS{PLC}Net-Sts") {
    field(DESC, "PLC Remote IO network status")
    field(SCAN, "1 second")
    field(DTYP, "EtherIP")
    field(INP, "@$(PLC,undefined) Misc_Sts_0[6] S 1")
    field(ZNAM,"Down")
    field(ONAM,"OK")
    field(ZSV,"MAJOR")
    field(OSV,"")
}

