#
#
#
## FET:DIGital?
# Read digital status bits.
#   0 Connected
#   1 Pending/Busy
#   2 Error
#   3 Reserved
#   4 Output changed (NOT SUPPORTED)
#   5 Reserved
#   6 Urgent device error (NOT SUPPORTED)
#   7 Reserved
#   8 Updating firmware
#   9 Starting up
#   10-15 Reserved
#   16  Acquisition Running (Measuring)
#   17  Acquisition Paused  (Waiting for trigger)
#   18  Acquisition Stopped
#   23  BNC gate toggling (Triggering)
# note: mbbiDirect is only 16-bit, need two records and bit shifting with calcout

record(longin, "$(Sys,undefined)$(Dev,undefined)DigStat-I") {
  field(DESC, "Digital status information")
  field(DTYP, "stream")
  field(SCAN, "Passive")
  field(PINI, "$(PINI,recursive)")
  #field(PINI, "YES")
  field(INP, "@F460.protocol readInt($(Sys,undefined)$(Dev,undefined),FET:DIG) $(PORT) 0")
  field(FLNK, "$(Sys,undefined)$(Dev,undefined)DigCalc1")
}

record(calcout, "$(Sys,undefined)$(Dev,undefined)DigCalc1") {
  field(INPA, "$(Sys,undefined)$(Dev,undefined)DigStat-I")
  field(CALC, "A & 0x0F")
  field(OUT, "$(Sys,undefined)$(Dev,undefined)DigStat1-I")
  field(OOPT, "On Change")
  field(FLNK, "$(Sys,undefined)$(Dev,undefined)DigCalc2")
}

record(calcout, "$(Sys,undefined)$(Dev,undefined)DigCalc2") {
  field(INPA, "$(Sys,undefined)$(Dev,undefined)DigStat-I")
  field(CALC, "(A >> 16) & 0x0F")
  field(OUT, "$(Sys,undefined)$(Dev,undefined)DigStat2-I")
  field(OOPT, "On Change")
}

record(mbbiDirect, "$(Sys,undefined)$(Dev,undefined)DigStat1-I") {
  field(DESC, "Lower byte of status")
  #field(FLNK, "$(Sys,undefined)$(Dev,undefined)DIG_STAT_1_FAN")
}

record(mbbiDirect, "$(Sys,undefined)$(Dev,undefined)DigStat2-I") {
  field(DESC, "Upper byte of status")
  #field(FLNK, "$(Sys,undefined)$(Dev,undefined)DIG_STAT_2_FAN")
}

