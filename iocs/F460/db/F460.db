  field(ONST, "Map compensated")
  field(TWST, "Mon A,B map X,Y")
  field(THST, "PID")
  field(INP, "@F460.protocol readInt($(Sys,undefined)$(Dev,undefined),OUT:MON) $(PORT) 0")
}

